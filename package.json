{
  "name": "sailfish-sdk",
  "version": "1.2.4",
  "description": "Sailfish SDK for JavaScript and TypeScript",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup --cjsInterop",
    "prepare": "npm run build",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:polymarket": "tsx test-polymarket-ws.ts",
    "validate-openapi": "redocly lint docs/openapi.yaml"
  },
  "keywords": [
    "sailfish",
    "sdk",
    "typescript",
    "javascript"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/axios": "^0.14.4",
    "@types/node": "^24.8.1",
    "cmd-ts": "^0.14.3",
    "dotenv": "^17.2.3",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.0",
    "tsx": "^4.19.2",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4",
    "@redocly/cli": "2.15.1"
  },
  "dependencies": {
    "axios": "^1.12.2",
    "isomorphic-ws": "^5.0.0"
  },
  "peerDependencies": {
    "axios": "^1.12.2"
  }
}